{
  "expo": {
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "name": "AwesomeProject",
    "slug": "folo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.u24web.folo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.u24web.folo",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "66d73cff-9684-40d3-88ff-557c8d65b0ac"
      }
    },
    "owner": "u24web"
  }
}
